#include <stdio.h>
/*
*_memset : function that set caracter in len string
* @s : string
* @c  : caracter
* @ len : size of caracter set in string
*return : return string 
*/

void	*_memset(void *b, int c, size_t len)
{
	size_t			i;
	unsigned char	*m;

	i = 0;
	m = (unsigned char *)b;
	while (i < len)
		m[i++] = (unsigned char)c;
	return (m);
}
